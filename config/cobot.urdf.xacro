<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="cobot">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import cobot urdf file -->
    <xacro:include filename="$(find cobot_simulation)/urdf/cobot.urdf.xacro" />

    <!-- Import control_xacro -->
    <xacro:include filename="cobot.ros2_control.xacro" />


    <xacro:cobot_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
